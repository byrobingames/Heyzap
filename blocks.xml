<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<palette>
    
    <block tag="initHeyzap" spec="Initialize Heyzap ads with 'Publisher ID': %0 [c:heyzap]" code="Heyzap.init(~);" help="Inititalize Heyzap ads at the begin of your project!" type="action" color="charcoal" returns="void">
        <fields>
            <text order="0"></text>
        </fields>
    </block>
    	
	<block tag="showInterstitialHezap" spec="Show Heyzap Interstitial with minimal Interval time: %0 minimal time to Call Before Display: %1  [c:heyzap]" code="Heyzap.showInterstitial(~,~);" help="Minimal Interval time in seconds, 'Show Heyzap Interstitial' cannot been called in this time. - Minimal time in seconds before display Interstitial ad after 'Show Heyzap Interstitial' is called " type="action" color="charcoal" returns="void">
        <fields>
		<number order="0"></number>
		<number order="1"></number>
        </fields>
    </block>
       
    <block tag="getInterstitialInfoHeyzap" spec="Heyzap Interstitial ad %0 [c:heyzap]" code="Heyzap.getInterstitialInfo(~)" type="normal" color="charcoal" returns="boolean">
        <fields>
            <dropdown order="0">
				<choices>
					<c text="did successfully load" code="0"></c>
					<c text="did fail to load" code="1"></c>
					<c text="is Closed" code="2"></c>
					<c text="is clicked" code="3"></c>
				</choices>
			</dropdown>
        </fields>
    </block>

</palette>